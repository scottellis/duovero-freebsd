From 35707c4570848e56263cf1821434391a6fd85458 Mon Sep 17 00:00:00 2001
From: Scott Ellis <scott@jumpnowtek.com>
Date: Wed, 14 Jan 2015 14:50:38 -0500
Subject: [PATCH 1/3] dts: omap443x: Remove unneeded address-cells prop from
 gic

---
 sys/boot/fdt/dts/arm/omap443x.dtsi | 6 +++---
 1 file changed, 3 insertions(+), 3 deletions(-)

diff --git sys/boot/fdt/dts/arm/omap443x.dtsi sys/boot/fdt/dts/arm/omap443x.dtsi
index 743d4b3..19ddd8c 100644
--- sys/boot/fdt/dts/arm/omap443x.dtsi
+++ sys/boot/fdt/dts/arm/omap443x.dtsi
@@ -29,6 +29,7 @@
  */
 
 / {
+	/* these next two come from skeleton.dtsi in Linux omap4.dtsi */
 	#address-cells = <1>;
 	#size-cells = <1>;
 
@@ -38,10 +39,9 @@
 	gic: interrupt-controller@48241000 {
 		compatible = "arm,gic";
 		interrupt-controller;
-		#address-cells = <0>;
 		#interrupt-cells = <1>;
-		reg =	< 0x48241000 0x1000 >,	/* Distributor Registers */
-			< 0x48240100 0x0100 >;	/* CPU Interface Registers */
+		reg = <0x48241000 0x1000>,
+		      <0x48240100 0x0100>;
 	};
 
 	soc: omap4430 {
