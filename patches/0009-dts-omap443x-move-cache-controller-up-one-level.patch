From 4ff8e702cff9afdce947c9cf716bd23001f20fc3 Mon Sep 17 00:00:00 2001
From: Scott Ellis <scott@jumpnowtek.com>
Date: Wed, 14 Jan 2015 15:28:55 -0500
Subject: [PATCH 2/3] dts: omap443x: Move cache controller up a level

---
 sys/boot/fdt/dts/arm/omap443x.dtsi | 14 +++++++-------
 1 file changed, 7 insertions(+), 7 deletions(-)

diff --git sys/boot/fdt/dts/arm/omap443x.dtsi sys/boot/fdt/dts/arm/omap443x.dtsi
index 19ddd8c..80b54cf 100644
--- sys/boot/fdt/dts/arm/omap443x.dtsi
+++ sys/boot/fdt/dts/arm/omap443x.dtsi
@@ -44,6 +44,13 @@
 		      <0x48240100 0x0100>;
 	};
 
+	L2: l2-cache-controller@48242000 {
+		compatible = "arm,pl310-cache", "arm,pl310";
+		reg = <0x48242000 0x1000>;
+		interrupts = <32>;
+		cache-level = <2>;
+	};
+
 	soc: omap4430 {
 		#address-cells = <1>;
 		#size-cells = <1>;
@@ -59,13 +66,6 @@
 				0x4a30a000 0x0600 >;
 		};
 				     
-		pl310@48242000 {
-			compatible = "arm,pl310";
-			reg = < 0x48242000 0x1000 >;
-			interrupts = < 32 >;
-			interrupt-parent = < &gic >;
-		};
-
 		mp_tmr@48240200 {
 			compatible = "arm,mpcore-timers";
 			#address-cells = <1>;
